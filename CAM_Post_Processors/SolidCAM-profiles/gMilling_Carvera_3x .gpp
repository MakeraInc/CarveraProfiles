    sHomestrtc            = 'x3:G00:Z-3;'                           ; Tool Change Homing Definition
    sHomestrend           = 'x3:G00:Z-3;3:G00:X-10Y-10'                  ; Program End Homing Definition
    sHomestrstpsame       = '3:Z;X0Y0 '                 ; M00 Same Tool Homing Definition
